<!DOCTYPE html>
<html lang="en">
<%- include("partials/header") %>

<body>

  <%- include("partials/navbar", { login: true }) %>


  <div class="container mt-5">
    <% usuarios.forEach(elementa => { %>
    <div class="row">
      <div class="col-4"></div>
      <div class="col-4">
        <div class="row border border-primary p-2 mb-3 rounded">
          <div class="col-3 d-flex justify-content-center align-items-center"><%- elementa.firstName %></div>
          <div class="col-6 pl-4">
            <div class="row small text-secondary">
              Apellido: <%- elementa.lastName %>
              Email: <%- elementa.email %>
            </div>
          </div>
          <div class="col-1"></div>
          <div class="col-2 d-flex justify-content-between align-items-center">
            <a href="/api/getUserToken"><i class="fas fa-user-secret"></i></a>
          </div>
        </div>

      </div>
      <div class="col-4"></div>
    </div>
    <% }) %>
  </div>



  <div><%- include("partials/footer") %></div>
</body>

</html>